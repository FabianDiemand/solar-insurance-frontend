{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 1%, not dead",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ],
    [
      "babel-plugin-root-import",
      {
        "rootPathPrefix": "@/",
        "rootPathSuffix": "src"
      }
    ]
  ]
}
